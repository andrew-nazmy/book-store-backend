{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "## Getting Started",
  "main": "server.ts",
  "scripts": {
    "dev_env":"set ENV=dev",
    "start": "set ENV=dev&& nodemon dist/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "tsc": "tsc",
    "migrateup": "db-migrate up ",
    "migratedown": "db-migrate reset",
    "jasmine": "set ENV=test&&jasmine --config=spec/support/jasmine.json  && db-migrate --env test reset",
    "test_env": " set ENV=test&& db-migrate --env test up "

  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.8.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.10.7",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.12",
    "jasmine": "4.5",
    "jasmine-spec-reporter": "7.0",
    "jasmine-ts": "^0.3.0",
    "supertest": "^6.3.3",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
